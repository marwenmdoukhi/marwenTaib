<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no">
    <title>Fashion for men and women </title>
    <link href="{{ asset('backadmin/plugins/file-upload/file-upload-with-preview.min.css') }}" rel="stylesheet" type="text/css" />
    {% block stylesheets %}
        <link href="{{ asset('build/css/admin.css') }}" rel="stylesheet">
    {% endblock %}
</head>
<body>
<div class="header-container fixed-top">
    <header class="header navbar navbar-expand-sm">
        <ul class="navbar-item theme-brand flex-row  text-center">
            <li class="nav-item theme-logo">
                <a href="{{ path('admin_dash') }}">
                    <img src="{{ asset('backadmin/img/logo.png') }}" width="100" height="100" class="navbar-logo" alt="logo">
                </a>
            </li>
            <li class="nav-item theme-text">
                <a href="#" class="nav-link"> Fashion for men and women </a>
            </li>
        </ul>

        <ul class="navbar-item flex-row ml-md-auto">

            <li class="nav-item dropdown user-profile-dropdown">
                <a href="javascript:void(0);" class="nav-link dropdown-toggle user" id="userProfileDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                    {% if app.user.picture is empty %}
                    <img src="{{asset('uploads/users/'~app.user.picture)}}" class="rounded-circle" alt="" />
                    {% else %}
                        <img src="{{ asset('backadmin/img/admin.jpg') }}" width="100" height="100" class="navbar-logo" alt="logo">

                    {% endif %}
                </a>
                <div class="dropdown-menu position-absolute" aria-labelledby="userProfileDropdown">
                    <div class="">
                        <div class="dropdown-item">
                            <a href="{{ path('admin_account_profil') }}"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-user"><path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path><circle cx="12" cy="7" r="4"></circle></svg> My Profile</a>
                        </div>

                        <div class="dropdown-item">
                            <a href="{{ path('admin_user_password') }}">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-lock"><rect x="3" y="11" width="18" height="11" rx="2" ry="2"></rect><path d="M7 11V7a5 5 0 0 1 10 0v4"></path></svg>
                                Mot de Passe</a>
                        </div>
                        <div class="dropdown-item">
                            <a href="{{path('admin_account_logout') }}"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-log-out"><path d="M9 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h4"></path><polyline points="16 17 21 12 16 7"></polyline><line x1="21" y1="12" x2="9" y2="12"></line></svg> Sign Out</a>
                        </div>
                    </div>
                </div>
            </li>

        </ul>
    </header>
</div>
<!--  END NAVBAR  -->

<!--  BEGIN NAVBAR  -->
<div class="sub-header-container">
    <header class="header navbar navbar-expand-sm">
        <a href="javascript:void(0);" class="sidebarCollapse" data-placement="bottom"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-menu"><line x1="3" y1="12" x2="21" y2="12"></line><line x1="3" y1="6" x2="21" y2="6"></line><line x1="3" y1="18" x2="21" y2="18"></line></svg></a>
    </header>
</div>
<!--  END NAVBAR  -->

<!--  BEGIN MAIN CONTAINER  -->
<div class="main-container" id="container">

    <div class="overlay"></div>
    <div class="search-overlay"></div>

    <!--  BEGIN SIDEBAR  -->
    <div class="sidebar-wrapper sidebar-theme">

        <nav id="sidebar">
            <div class="shadow-bottom"></div>
            <ul class="list-unstyled menu-categories" id="accordionExample">

                <li class="menu">
                    <a href="{{ path('admin_dash') }}"  data-active={% if current_link is defined and current_link == 'dashbord' %}true{% endif %}" aria-expanded="{% if current_link is defined and current_link == 'dashbord' %}true{% endif %}" class="dropdown-toggle  {% if current_link is defined and current_link == 'dashbord' %} text-light {% endif %}">
                        <div class="">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-home"><path d="M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z"></path><polyline points="9 22 9 12 15 12 15 22"></polyline></svg>
                            <span>Dashboard</span>
                        </div>
                    </a>
                </li>

                <li class="menu">
                    <a href="{{ path('user_inscri') }}"  data-active={% if current_link is defined and current_link == 'user' %}true{% endif %}" aria-expanded="{% if current_link is defined and current_link == 'user' %}true{% endif %}" class="dropdown-toggle  {% if current_link is defined and current_link == 'user' %} text-light {% endif %}">
                        <div class="">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-home"><path d="M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z"></path><polyline points="9 22 9 12 15 12 15 22"></polyline></svg>
                            <span>User Inscri</span>
                        </div>
                    </a>
                </li>

                <li class="menu">
                    <a href="{{ path('demande') }}"  data-active={% if current_link is defined and current_link == 'demande' %}true{% endif %}" aria-expanded="{% if current_link is defined and current_link == 'demande' %}true{% endif %}" class="dropdown-toggle  {% if current_link is defined and current_link == 'demande' %} text-light {% endif %}">
                        <div class="">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-clipboard"><path d="M16 4h2a2 2 0 0 1 2 2v14a2 2 0 0 1-2 2H6a2 2 0 0 1-2-2V6a2 2 0 0 1 2-2h2"></path><rect x="8" y="2" width="8" height="4" rx="1" ry="1"></rect></svg>
                            <span>Demande</span>
                        </div>
                    </a>
                </li>
                <li class="menu">
                    <a href="{{ path('paiement') }}"  data-active={% if current_link is defined and current_link == 'paiement' %}true{% endif %}" aria-expanded="{% if current_link is defined and current_link == 'paiement' %}true{% endif %}" class="dropdown-toggle  {% if current_link is defined and current_link == 'paiement' %} text-light {% endif %}">
                        <div class="">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-home"><path d="M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z"></path><polyline points="9 22 9 12 15 12 15 22"></polyline></svg>
                            <span>Facture</span>
                        </div>
                    </a>
                </li>
                <li class="menu">
                    <a href="#dashboard" data-active={% if current_link is defined and current_link == 'produit' %}true{% endif %}" aria-expanded="{% if current_link is defined and current_link == 'produit' %}true{% endif %}"  data-toggle="collapse"  class="dropdown-toggle">
                        <div class="">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-home"><path d="M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z"></path><polyline points="9 22 9 12 15 12 15 22"></polyline></svg>
                            <span>Produit</span>
                        </div>
                        <div>
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-chevron-right"><polyline points="9 18 15 12 9 6"></polyline></svg>
                        </div>
                    </a>
                    <ul class="collapse submenu list-unstyled" id="dashboard" data-parent="#accordionExample">
                        <li >
                            <a href="{{ path('product_index') }}"  data-active={% if current_link is defined and current_link == 'produit' %}true{% endif %}" aria-expanded="{% if current_link is defined and current_link == 'produit' %}true{% endif %}" class="dropdown-toggle  {% if current_link is defined and current_link == 'produit' %} text-light {% endif %}">
                                <div class="">
                                    <span>Liste Produit</span>
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="{{ path('category_index') }}"  data-active={% if current_link is defined and current_link == 'categories' %}true{% endif %}" aria-expanded="{% if current_link is defined and current_link == 'categories' %}true{% endif %}" class="dropdown-toggle  {% if current_link is defined and current_link == 'categories' %} text-light {% endif %}">
                                <div class="">
                                    <span>Liste Categorie</span>
                                </div>
                            </a>
                        </li>

                        <li>
                            <a href="{{ path('sub_category_index') }}"  data-active={% if current_link is defined and current_link == 'subcategories' %}true{% endif %}" aria-expanded="{% if current_link is defined and current_link == 'subcategories' %}true{% endif %}" class="dropdown-toggle  {% if current_link is defined and current_link == 'subcategories' %} text-light {% endif %}">
                                <div class="">
                                    <span>Liste Sous Categorie</span>
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="{{ path('subsubcategories_index') }}"  data-active={% if current_link is defined and current_link == 'subsubcategories' %}true{% endif %}" aria-expanded="{% if current_link is defined and current_link == 'subsubcategories' %}true{% endif %}" class="dropdown-toggle  {% if current_link is defined and current_link == 'subsubcategories' %} text-light {% endif %}">
                                <div class="">
                                    <span>Sous Sous categories</span>
                                </div>
                            </a>
                        </li>
                    </ul>
                </li>

                <li class="menu">
                    <a href="{{ path('marque_index') }}"  data-active={% if current_link is defined and current_link == 'marques' %}true{% endif %}" aria-expanded="{% if current_link is defined and current_link == 'marques' %}true{% endif %}" class="dropdown-toggle  {% if current_link is defined and current_link == 'marques' %} text-light {% endif %}">
                        <div class="">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-box"><path d="M21 16V8a2 2 0 0 0-1-1.73l-7-4a2 2 0 0 0-2 0l-7 4A2 2 0 0 0 3 8v8a2 2 0 0 0 1 1.73l7 4a2 2 0 0 0 2 0l7-4A2 2 0 0 0 21 16z"></path><polyline points="3.27 6.96 12 12.01 20.73 6.96"></polyline><line x1="12" y1="22.08" x2="12" y2="12"></line></svg>
                            <span>Listes Des marques</span>
                        </div>
                    </a>
                </li>
                <li class="menu">
                    <a href="{{ path('news_index') }}"  data-active={% if current_link is defined and current_link == 'news' %}true{% endif %}" aria-expanded="{% if current_link is defined and current_link == 'news' %}true{% endif %}" class="dropdown-toggle  {% if current_link is defined and current_link == 'news' %} text-light {% endif %}">
                        <div class="">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-file"><path d="M13 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V9z"></path><polyline points="13 2 13 9 20 9"></polyline></svg>
                            <span>News  </span>
                        </div>
                    </a>
                </li>
            </ul>
        </nav>
    </div>
    <div id="content" class="main-content">
        <div class="layout-px-spacing">
            {% block body %}
            {% endblock %}
        </div>
    </div>
</div>

<script src="{{ asset('backadmin/assets/js/libs/jquery-3.1.1.min.js') }}"></script>
<script src="{{ asset('backadmin/bootstrap/js/popper.min.js') }}"></script>
<script src="{{ asset('backadmin/bootstrap/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('backadmin/plugins/table/datatable/datatables.js') }}"></script>
<script src="{{ asset('backadmin/plugins/apex/apexcharts.min.js') }}"></script>
<script src="{{ asset('backadmin/assets/js/dashboard/dashbord.js') }}"></script>
<script src="{{ asset('backadmin/file-upload/file-upload-with-preview.min.js') }}"></script>

<script>
    $(document).ready(function() {
        App.init();
    });
</script>
{% block javascripts %}
    <script src="{{ asset('build/js/admin.js') }}"></script>
{% endblock %}
</body>
</html>